{
  "expo": {
    "name": "CookSmart",
    "slug": "CookSmart",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/imgg.png",
    "scheme": "cooksmart",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.altheoz06.CookSmart"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/imgg.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "1e93168b-743a-45e9-a52f-f8777000a958"
      },
      "EXPO_PUBLIC_GEMINI_API_KEY": "AIzaSyCm_DK3E0bVuJ1fOhvM23WbRgM4EBUrg5s"
    }
  }
}
